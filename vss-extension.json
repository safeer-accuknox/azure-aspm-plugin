{
    "manifestVersion": 1,
    "id": "safeer-accuknox-SAST",
    "name": "AccuKnox SAST",
    "version": "1.0.2",
    "publisher": "safeer-accuknox",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "AccuKnox SAST extension for scanning source code.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/logo.png"
    },
    "files": [
        {
            "path": "SAST"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "safeer-accuknox-SAST",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SAST"
            }
        }
    ],
    "public": false,
    "content": {
        "details": {
            "path": "overview.md"
        }
    }
}