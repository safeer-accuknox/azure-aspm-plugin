{
    "manifestVersion": 1,
    "id": "safeer-accuknox-iac",
    "name": "AccuKnox IAC",
    "version": "0.1.21",
    "publisher": "safeer-accuknox",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "AccuKnox IAC task for scanning web applications.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/logo.png"        
    },
    "files": [
        {
            "path": "src"
        }
    ],
    "contributions": [
        {
            "id": "safeer-accuknox-iac",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "src"
            }
        }
    ],
    "public": false,
    "content": {
        "details": {
            "path": "overview.md"
        }
    }
}