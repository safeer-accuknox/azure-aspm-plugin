{
    "manifestVersion": 1,
    "id": "safeer-accuknox-secret-scan",
    "name": "AccuKnox Secret Scan",
    "version": "0.0.4",
    "publisher": "safeer-accuknox",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "AccuKnox Secret Scan task designed to scan and identify secrets in Git repositories.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/logo.png"        
    },
    "files": [
        {
            "path": "src"
        }
    ],
    "contributions": [
        {
            "id": "safeer-accuknox-secret-scan",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "src"
            }
        }
    ],
    "public": false,
    "content": {
        "details": {
            "path": "overview.md"
        }
    }
}